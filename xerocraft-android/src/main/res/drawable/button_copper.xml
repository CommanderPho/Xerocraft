<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid android:color="#743b26" />
            <stroke
                android:width="1dp"
                android:color="#7f7f7f" />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#f0d7c6"
                android:endColor="#743b26"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#7f7f7f" />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
</selector>